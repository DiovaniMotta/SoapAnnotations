
import org.soap.diovani.motta.transactions.Transactions;
import org.soap.diovani.motta.transactions.TransactionsPrimitive;


public class Main {

	public static void main(String[] args) {
		//Boolean boolean1 = new Boolean(s);
		Transactions transactions = new TransactionsPrimitive();
		transactions.addURL("http://10.0.0.103:8080/Teste/services/Serial?wsdl");
		transactions.addNAMESPACE("http://teste");
		try {
			boolean retorno = transactions.call(Boolean.class, "salvar2");
			System.out.println(retorno);
		} catch (Exception e) {
			// TODO Auto-generated catch block
			e.printStackTrace();
		}
	}

}
